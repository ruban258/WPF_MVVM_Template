<UserControl x:Class="HMI.MVVM.View.HomeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HMI.MVVM.View"
             xmlns:controls="clr-namespace:CustomControls;assembly=CustomControls"
             mc:Ignorable="d"
             Background="DarkGray"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <controls:IOField MaxValue="20" Width="120" Height="20" IsReadOnly="True"/>
        <controls:IOField MaxValue="100" Width="120" Height="20" IsReadOnly="True"/>
        <controls:IOField MaxValue="200" Width="120" Height="20" IsReadOnly="True"/>
    </StackPanel>
</UserControl>
